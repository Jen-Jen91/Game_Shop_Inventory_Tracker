<h1>All Publishers</h1>

<button type="button">
  <a href="/publishers/new">Add New Publisher</a>
</button>

<section class="index">
  <hr>
  <% for publisher in @publishers %>

  <p>Name: <a href="/publishers/<%= publisher.id %>"><%= publisher.name %></a></p>
  <br>
  <p>Contact Details</p>
  <p>Name: <%= publisher.contact_name %></p>
  <p>Email: <%= publisher.contact_email %></p>
  <hr>

  <% end %>
</section>
